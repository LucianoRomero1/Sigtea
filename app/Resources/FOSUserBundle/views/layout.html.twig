    </head>
    
    <body>
        <nav class="navbar navbar-fixed-top login-nav">
            <div class="container row-top">

                    <div class="navbar-header">
                        <a href="{{path('fos_user_security_login')}}"><img src="https://www.santafe.gob.ar/assets/standard/images/gob-santafe.png"></a>
                    </div>

                    <div class="media" style="float: right;">

                        <a class="btn-circle" href="#"><i class="fab fa-twitter nav-icon"></i></a>
                        <a class="btn-circle" href="#"><i style="margin-left: 9px;" class="fab fa-facebook-f nav-icon"></i></a>
                        <a class="btn-circle" href="#"><i style="margin-left: 5px;" class="fab fa-youtube nav-icon"></i></a>
                        
                    </div>
            </div>
        </nav>
        {# <div>
            {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
                <a href="{{ path('fos_user_security_logout') }}">
                    {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                </a>
            {% else %}
                <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>
            {% endif %}
        </div> #}

        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}
                    <div class="flash-{{ type }}">
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}

        <div>
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>
    </body>
