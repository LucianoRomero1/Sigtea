{#% extends 'base.html.twig' %#}
{% block content %}
    <style type="text/css">
        .ui-icon-error {
            background-position: -98px -128px;
        }
    </style>
    {% block flash_messages %}
        {% set flash_messages = app.session.flashbag.all %}
        {% if flash_messages|length > 0 %}
            {% for label, flashes in flash_messages %}
                {% for flash in flashes %}
                    <div>
                        <div class="ui-widget">
                            <div style="padding: 10px; margin-top: 20px; margin-bottom: 20px" class="ui-state-error ui-corner-all">
                                <p>
                                    <span style="float: left; margin-right: 0.3em;" class="ui-icon ui-icon-{{ label }}"> </span>
                                    {% block mensajeFlash %} {{ flash | raw }} {% endblock %}
                                </p>
                            </div>
                        </div>
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}
    {% endblock %}
